<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("./partials/head") %>
    <title>Document</title>
</head>
<body>
    <%- include("./partials/navbar") %>
    <% blogs.forEach(blog => { %>
     <div class="container mt-3">
        <div class="card" style="width: 18rem;">
            <img src='<%= blog.coverImg %>' class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%= blog.title %></h5>
              <a href="/blog/<%= blog._id %>" class="btn btn-primary">View</a>
            </div>
          </div>
     </div>
    <% }) %>
    <%- include("./partials/scripts") %> <!--automatically import the code in scripts.ejs-->
</body>
</html>